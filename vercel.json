{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/health",
      "destination": "/booking-backend/src/server.ts"
    },
    {
      "source": "/api/calendar/(.*)",
      "destination": "/api/calendar/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}